{
    "name": "tunecino/yii2-app-builder",
    "description": "Fork of Yii 2 Advanced Project Template With RESTful API, OAuth 2.0 and Schema Builder",
    "keywords": ["yii2", "framework", "advanced", "project template", "builder", "rest", "bearer", "rest authentication"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/tunecino/yii2-app-builder/issues",
        "wiki": "https://github.com/tunecino/yii2-app-builder/wiki",
        "source": "https://github.com/tunecino/yii2-app-builder"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0@dev",
        "yiisoft/yii2-bootstrap": "*",
        "tunecino/yii2-schema-builder": "^0.1.5",
        "dmstr/yii2-adminlte-asset": "^2.6",
        "dixonsatit/yii2-agency-theme": "^1.0",
        "mootensai/yii2-uuid-behavior": "^1.0",
        "wbraganca/yii2-dynamicform": "^2.0",
        "kartik-v/yii2-widget-select2": "^2.1",
        "mootensai/yii2-enhanced-gii": "dev-master",
        "mootensai/yii2-relation-trait": "^1.1",
        "kartik-v/yii2-mpdf": "dev-master",
        "deesoft/yii2-gii": "^1.4",
        "meysampg/yii2-treeview": "^1.0",
        "yiisoft/yii2-jui": "^2.0",
        "mdmsoft/yii2-admin": "^2.8",
        "kartik-v/yii2-tree-manager": "dev-master",
        "phpoffice/phpspreadsheet": "^1.3",
        "kartik-v/yii2-datecontrol": "^1.9",
        "yiisoft/yii2-swiftmailer": "*",
        "bizley/migration": "^3.0",
        "jamband/yii2-schemadump": "^0.10.0",
        "kartik-v/yii2-social": "@dev",
        "chiliec/yii2-vote": "^4.2",
        "machour/yii2-notifications": "^1.0@dev",
        "2amigos/yii2-chartjs-widget": "^2.1",
        "kartik-v/yii2-widget-datetimepicker": "dev-master",
        "philippfrenzel/yii2fullcalendar": "*",
        "roromix/spreadsheetbundle": "dev-master",
        "yiisoft/yii2-twig": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "sebastian/object-reflector": "^1.1@dev"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "pattern-skip-version": "(-build|-patch)"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
